#include 'TOTVS.ch'

class cbcCusProd
    data cTipo
    data nValor
    method newcbcCusProd() constructor 
    method setTipo()
    method setValor()
    method getValor()
endClass

method newcbcCusProd(cTipo, nValor) class cbcCusProd
    ::cTipo     := cTipo
    ::nValor    := nValor
return(self)


method setTipo(cTipo) class cbcCusProd
    ::cTipo := cTipo
return self


method setValor(nValor) class cbcCusProd
    ::nValor := nValor
return self


method getValor() class cbcCusProd
return ::nValor

