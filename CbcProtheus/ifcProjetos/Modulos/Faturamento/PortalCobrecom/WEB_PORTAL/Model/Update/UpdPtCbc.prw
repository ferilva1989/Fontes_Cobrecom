	'S'																		, ; //X7_SEEK
	'SX5'																	, ; //X7_ALIAS
	1																		, ; //X7_ORDEM
	'xFilial("SX5")+"_1"+M->ZP8_GRUPO'										, ; //X7_CHAVE
	'U'																		, ; //X7_PROPRI
	''																		} ) //X7_CONDIC

//
// Campo ZPB_CODUSR
//
aAdd( aSX7, { ;
